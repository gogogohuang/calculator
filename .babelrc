{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            "styled-jsx-plugin-sass"
          ]
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "components": "./src/components",
          "pages": "./src/pages",
          "hooks": "./src/hooks",
          "server": "./src/server",
          "styles": "./src/styles",
          "utils": "./src/utils",
          "context": "./src/context"
        }
      }
    ]
  ]
}